import math
import sys

def factorize(number):
    # Find the smallest factor greater than or equal to sqrt(number)
    max_factor = math.isqrt(number)
    for factor in range(max_factor, 1, -1):
        if number % factor == 0:
            # Found a factor, return the factorization
            return f"{number}={factor}*{number//factor}"
    # If no factor is found, the number is prime
    return f"{number}=1*{number}"

if len(sys.argv) != 2:
    print("Usage: factors <file>")
    sys.exit(1)

with open(sys.argv[1]) as f:
    for line in f:
        number = int(line.strip())
        print(factorize(number))
